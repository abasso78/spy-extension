{
  "name": "spy-extension",
  "version": "2.0.0",
  "description": "The most invasive Chrome extension ever",
  "scripts": {
    "start": "parcel watch src/manifest.json --host localhost --config @parcel/config-webextension --no-content-hash --no-cache",
    "build": "parcel build src/manifest.json --config @parcel/config-webextension --no-content-hash --no-cache --no-scope-hoist && node scripts/update-dist-manifest-importmap-hash.js",
    "build:prod": "cross-env NODE_ENV=production parcel build src/manifest.json --config @parcel/config-webextension --dist-dir dist --public-url ./ --no-scope-hoist && node scripts/update-dist-manifest-importmap-hash.js",
    "analyze": "cross-env NODE_ENV=production parcel build src/manifest.json --config @parcel/config-webextension --dist-dir dist --public-url ./ --no-scope-hoist --reporter @parcel/reporter-bundle-analyzer",
    "test": "jest",
    "clean": "rm -r .parcel-cache dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msfrisbie/spy-extension.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/msfrisbie/spy-extension#readme",
  "devDependencies": {
    "@parcel/config-webextension": "^2.16.0",
    "@parcel/reporter-bundle-analyzer": "^2.16.0",
    "@parcel/transformer-sass": "^2.16.0",
    "@types/chrome": "^0.0.216",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.16.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  }
}
